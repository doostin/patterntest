{
    "rules": {
        "indent": [2, "tab"],
        "quotes": [2, "double"],
        "linebreak-style": [2, "unix"],
        "semi": [2, "always"],
        "no-console": 1,
        "comma-dangle": [2, "never"],
        "eqeqeq": [2, "allow-null"],
        "vars-on-top": 2,
        "camelcase": [0, {"properties": "always"}]
    },
    "env": {
        "node": true
    },
    "extends": "airbnb/legacy"
}
